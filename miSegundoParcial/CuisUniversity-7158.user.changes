!Pintor methodsFor: 'alineacion de pinturas' stamp: 'MN 7/4/2025 21:07:19' prior: 50581033!
alinearADerechaPinturasSeleccionadas
	
	| posicionXDePinturaMasADerecha |
	
	self assertHayMasDeUnaPinturaSeleccionadaParaAlinear.
	
	accionesARehacer := OrderedCollection new.
	accionesHechas add: [:unPintor| unPintor alinearADerechaPinturasSeleccionadas].
	
	posicionXDePinturaMasADerecha := pinturasSeleccionadas
		max: [:cadaPintura| cadaPintura corner x].
		
	pinturasSeleccionadas do:[:cadaPintura|
		| nuevoOrigen |
		nuevoOrigen := ((posicionXDePinturaMasADerecha - (cadaPintura extent x))@(cadaPintura origin y)).
		cadaPintura
			setOrigin: nuevoOrigen
			corner: (nuevoOrigen + (cadaPintura extent)).
	].! !
